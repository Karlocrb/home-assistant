{
    "name": "Webhook Relay",
    "version": "1.3.0",
    "slug": "webhook-relay",
    "description": "Receive webhooks without public IP or configuring NAT. Account registration required.",
    "url": "https://webhookrelay.com/v1/guide/home-automation.html#Home-Assistant",    
    "startup": "application",
    "host_network": false,
    "map": ["ssl:rw"],
    "arch": [
        "aarch64", 
        "amd64",
        "armhf"
    ],
    "boot": "auto",
    "options": {
        "key": null,
        "secret": null,
        "forwarding": [
            {
                "bucket": "ha",
                "destination": "http://hassio:8123"
            }
        ],
        "tunnels": [
            {
                "name": "HA",
                "subdomain": "",
                "protocol": "tls",
                "destination": "http://hassio:8123",
                "key_file": "",
                "cert_file": ""
            }
        ],
        "tunnels_enabled": false,
        "forwarding_enabled": false

      },
      "schema": {
        "key": "str",
        "secret": "str",     
        "forwarding": [
            {
                "bucket": "str",
                "destination": "url"
            }
        ],
        "tunnels": [
            {
                "name": "str",
                "protocol": "str",
                "subdomain": "str?",
                "domain": "str?",
                "key_file": "str?",
                "cert_file": "str?",                
                "destination": "url",
                "auto_gen": "bool?"
            }
        ],    
        "tunnels_enabled": "bool",
        "forwarding_enabled": "bool"
      }   
  }